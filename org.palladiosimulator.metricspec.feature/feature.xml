<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.metricspec.feature"
      label="Metric Specification Feature"
      version="1.0.0.qualifier"
      provider-name="palladiosimulator.org"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0.qualifier">

   <description url="http://palladiosimulator.org">
      Allows the definition of metrics for measurements.
   </description>

   <copyright>
      This plugin is protected by copyright held by the members of
the Palladio Research Group, University of Karlsruhe.
   </copyright>

   
   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="de.uka.ipd.sdq.identifier" version="2.1.0" match="greaterOrEqual"/>
      <import plugin="org.jscience" version="4.1.3" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.edit"/>
      <import plugin="de.uka.ipd.sdq.identifier.edit" version="2.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.ecore" version="2.9.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
      <import plugin="org.eclipse.emf.edit.ui"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.emf.ecore.edit"/>
   </requires>

   <plugin
         id="org.palladiosimulator.metricspec"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.metricspec.edit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.metricspec.resources"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="org.palladiosimulator.metricspec.editor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
