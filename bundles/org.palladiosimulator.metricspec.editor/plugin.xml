      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated metricspec -->
      <editor
            id="org.palladiosimulator.metricspec.presentation.MetricSpecEditorID"
            name="%_UI_MetricSpecEditor_label"
            icon="icons/full/obj16/MetricSpecModelFile.gif"
            extensions="metricspec"
            class="org.palladiosimulator.metricspec.presentation.MetricSpecEditor"
            contributorClass="org.palladiosimulator.metricspec.presentation.MetricSpecActionBarContributor">
      </editor>
