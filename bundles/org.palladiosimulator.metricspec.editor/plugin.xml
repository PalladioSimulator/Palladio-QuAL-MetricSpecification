<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated metricspec -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.palladiosimulator.metricspec.presentation.MetricSpecModelWizardID"
            name="%_UI_MetricSpecModelWizard_label"
            class="org.palladiosimulator.metricspec.presentation.MetricSpecModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MetricSpecModelFile.gif">
         <description>%_UI_MetricSpecModelWizard_description</description>
